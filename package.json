{
  "name": "@applitools/eyes-webdriverio",
  "version": "5.9.4",
  "description": "Applitools Eyes SDK for WebdriverIO",
  "keywords": [
    "eyes-webdriverio",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "webdriverio"
  ],
  "homepage": "https://applitools.com",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.webdriverio.javascript5.git"
  },
  "bugs": {
    "url": "https://github.com/applitools/eyes.webdriverio.javascript5/issues"
  },
  "directories": {
    "example": "./example",
    "lib": "./src",
    "test": "./test"
  },
  "main": "./index.js",
  "dependencies": {
    "@applitools/dom-utils": "~4.6.41",
    "@applitools/eyes-sdk-core": "^5.24.0",
    "@applitools/visual-grid-client": "^13.5.7",
    "chromedriver": "^79.0.0",
    "geckodriver": "^1.19.1",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "webdriverio": "^5.18.4"
  },
  "devDependencies": {
    "@applitools/sdk-test-kit": "^1.0.17",
    "eslint": "^6.8.0",
    "mocha": "^6.2.2",
    "mocha-parallel-tests": "^2.2.2",
    "shared-examples-for": "^0.1.3"
  },
  "scripts": {
    "lint": "eslint '**/*.js'",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "mocha --no-timeouts \"test/+(TestClassicApi*.js|TestFluentApi*.js|TestVisualGrid*.js)\"",
    "test:unit": "mocha \"test/unit/**/*.js\"",
    "test:coverage": "coverage-tests run --path test/coverage/index.js",
    "test:coverage:nuke": "coverage-tests nuke"
  },
